{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2024",
		"lib": [
			"ES2024"
		],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	],
	"include": [
		"src/**/*.ts",
		"../shared/**/*.ts",
		"../shared/config.json"
	]
}