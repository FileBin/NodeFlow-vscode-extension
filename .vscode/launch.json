{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"resolveSourceMapLocations": [
				"${workspaceFolder}/packages/extension/**",
				"!**/node_modules/**"
			],
			"args": [
				"${env:TMPDIR}/ext-test-workspace",
				"--extensionDevelopmentPath=${workspaceRoot}/packages/extension"
			],
			"outFiles": ["${workspaceFolder}/out/**/*.js"],
		}
	]
}